generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Purchase {
  id        String   @id @default(cuid())
  userId    String
  memeId    String
  createdAt DateTime @default(now())
}
